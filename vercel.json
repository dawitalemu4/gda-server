{
    "builds": [
        {
            "src": "src/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "runtime": "python3.9"        
            }
        },
        {
            "src": "build.sh",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "staticfiles_build"
            }
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "src/wsgi.py"
        }
    ]
}
